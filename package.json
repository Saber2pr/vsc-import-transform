{
	"name": "import-transform",
	"displayName": "Antd Import Transform",
	"description": "transform import like babel-plugin-import, for antd.",
	"publisher": "saber2pr",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Saber2pr/vsc-import-transform.git"
	},
	"icon": "assets/logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "import-transform.main",
				"title": "Import Transform"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "import-transform.main",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"build": "vsce package",
		"release": "standard-version"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"standard-version": "^9.3.2",
		"typescript": "^4.1.3"
	}
}